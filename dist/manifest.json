{
  "name": "Guaramientas",
  "version": "0.4.0",
  "description": "Herrramientas para Guaranpis",
  "manifest_version": 2,
  "permissions": ["activeTab", "declarativeContent", "downloads"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://guaranpis.es/*"],
      "js": ["content.js"]
    }
  ]
}
